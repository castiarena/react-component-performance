    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.ts{,x}": [
      "bash -t tsc --noEmit",
      "bash -t prettier --write --ignore-unknown",
      "bash -t eslint --fix",
      "yarn test"
    ]
  }
}
