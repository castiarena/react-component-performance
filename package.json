{
  "name": "react-component-performance",
  "version": "1.0.0",
  "description": "Express with SSR and react application",
  "engines": {
    "node": ">=12.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 dist/server.js",
    "start:dev": "nodemon -r ts-node app/index.ts",
    "build": "webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint app/**/*.ts{,x}",
    "format": "prettier --write --ignore-unknown app/**/*.ts{,x}"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "7.11.0",
